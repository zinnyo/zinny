<%- include('../partials/header') %>

<nav class="navbar navbar-light bg-light">
    <a class="navbar-brand" href="../<%=id%>">Zinny Gaming PC & Components Sale, Trade-in and Service</a>
  
  </nav>
  <br />

<h1 class="text-center text-primary">My Order List</h1>



<div class="container-fluid">
<table class="table table-dark table-bordered" style="margin-top: 20px;">
  <thead>
    <tr>

      <th scope="col">Code</th>
      <th scope="col">Product Name</th>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">Address</th>
      <th scope="col">Ph No</th>
      <th scope="col">Qty</th>
      <th scope="col">Cost</th>
      <th scope="col">Status</th>
      <th scope="col">Used Point</th>
      <th scope="col">Bonus Point</th>

  
      <!-- <th scope="col" colspan=""></th> -->
    </tr>
  </thead>
  <tbody>
    <% orders.forEach(function(order) { %>
    <tr>

    

      <td><%=order.code%></td>
      <td><%=order.productName%></td>
      <td><%=order.name%></td>
      <td><%=order.email %></td>
      <td><%=order.address%></td>
      <td><%=order.phNo%></td>
      <td><%=order.qty%></td>
      <td>$<%=order.cost%></td>
      <td><%=order.status%></td>
      <td><%=order.usedPoint%></td>
      <td><%=order.bonusPoint%></td>
    

     
     
     
   
    </tr>
    <% }) %>
  </tbody>
</table>

</div>
<%- include('../partials/footer') %>